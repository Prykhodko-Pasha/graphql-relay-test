            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/react-dom": "^17.0.11",
        "@types/react-relay": "^13.0.0",
        "@types/react-test-renderer": "^17.0.1",
